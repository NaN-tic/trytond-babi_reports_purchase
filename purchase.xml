            <field name="ttype">float</field>
            <field name="model" search="[('model','=','purchase.line')]"/>
            <field name="expression">o.product and o.product.forecast_quantity</field>
            <field name="name">Product Forecast Quantity</field>
        </record>
        <!--
             Reports
        -->
        <!-- purchase.purchase -->

        <!-- Year - Month - Day | Amount Untaxed -->
        <record model="babi.report" id="purchase_year_month_date">
            <field name="parent_menu" ref="babi.menu_babi"/>
            <field name="model" search="[('model','=','purchase.purchase')]"/>
            <field name="filter" ref="processing_done_purchases"/>
            <field name="name">Purchases by Year - Month - Day</field>
            <field name="timeout" eval="1200"/>
        </record>
        <record model="babi.report-res.group"
                id="purchase_year_month_date_group_babi_">
            <field name="report" ref="purchase_year_month_date"/>
            <field name="group" ref="babi.group_babi"/>
        </record>
        <record model="babi.dimension.column" id="purchase_year_month_date__year">
            <field name="report" ref="purchase_year_month_date"/>
            <field name="expression" ref="purchase_purchase_year"/>
            <field name="name">Year</field>
            <field name="sequence" eval="1"/>
        </record>
        <record model="babi.dimension" id="purchase_year_month_date__month">
            <field name="report" ref="purchase_year_month_date"/>
            <field name="expression" ref="purchase_purchase_month"/>
            <field name="name">Month</field>
            <field name="sequence" eval="2"/>
        </record>
        <record model="babi.dimension" id="purchase_year_month_date__date">
            <field name="report" ref="purchase_year_month_date"/>
            <field name="expression" ref="purchase_purchase_day"/>
            <field name="name">Day</field>
            <field name="sequence" eval="3"/>
        </record>
        <record model="babi.measure" id="purchase_year_month_date__untaxed_amount">
            <field name="report" ref="purchase_year_month_date"/>
            <field name="expression" ref="purchase_purchase_untaxed_amount"/>
            <field name="name">Untaxed Amount</field>
